@model IEnumerable<SabongPinoy.Models.v_LoginIP>
@{
    ViewBag.Title = "IpAdd";
}

<div id="home">
    <div class="container">
        <div id="services">
            <div class="row">
                <div class="col-sm-10 col-sm-offset-1 col-md-10 col-md-offset-1 col-lg-10 col-lg-offset-1 ">
                    <div class="panel panel-primary">
                        <div class="panel-heading text-left">
                            <i class="ion-ios-people"></i> Load Points
                            <div class="pull-right">
                                <div class="btn-group open">
                                    <a class=" btn btn-default btn-xs" href="@Url.Action("Users", "Admin")" title="Go Back"><i class="ion ion-arrow-left-b fa-fw"></i></a>
                                </div>
                            </div>
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                            <div class="form-wrapper">
                                <h3 class="text-left text-custom-one">
                                    <span class="text-info"> Username: </span>  @Request.QueryString["un"]
                                </h3>
                                <hr />

                                <div class=" table-responsive">
                                    <table class="table table-striped table-hover text-dark-space text-left small">
                                        <thead class="text-center">
                                            <tr>
                                                <th>Date Login</th>
                                                <th>IP</th>

                                            </tr>
                                        </thead>
                                        <tbody>

                                            @foreach (var item in Model)
                                            {
                                                <tr>
                                                    <td>
                                                        @item.DateLogin
                                                    </td>
                                                    <td>
                                                        @item.IPAddress
                                                    </td>

                                                </tr>
                                            }


                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                        <!-- /.panel-body -->
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>

