    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="~/Content/clock.css" rel="stylesheet" />
    <script src="~/Scripts/jquery.signalR-2.4.1.min.js"></script>
    <script src="~/signalr/hubs"></script>
    <script src="~/Scripts/jquery.noty.packaged.min.js"></script>
    <script src="~/Scripts/arena.js?v=7"></script>
    <script src="~/Scripts/jquery.maskedinput.js"></script>
    <script type="text/javascript">
        function reverseOdd(yourString) {
            var n = yourString.toString().split("-");
            return n[1] + '-' + n[0];
        }
        $(document).ready(
            function () {
                $('#lnkViewPayout').click(function () {
                    var refNum = $(this).attr('href');
                    if (refNum != '') {
                        var x = screen.width / 2 - 700 / 2;
                        var y = screen.height / 2 - 450 / 2;
                        window.open(refNum, 'Payout', 'height=485,width=700,left=' + x + ',top=' + y);
                    }
                    return false;
                });

                $('#lnkViewTotal').click(function () {
                    var refNum = $(this).attr('href');
                    if (refNum != '') {
                        var x = screen.width / 2 - 700 / 2;
                        var y = screen.height / 2 - 450 / 2;
                        window.open(refNum, 'Total', 'height=485,width=700,left=' + x + ',top=' + y);
                    }
                    return false;
                });


                $("#txtBetAmountZero").mask("#,###,###", { reverse: true });


                setInterval(function () {

                    var tdTotalBetDraw = $("#tdTotalBetDraw-8").text()
                    var tdPersonalBetDraw = $("#tdPersonalBetDraw-8").text()


                    $("#draw-total").text(tdTotalBetDraw)
                    $("#draw-personal").text(tdPersonalBetDraw)



                }, 500);


                displayBaccaratResult()

                setInterval(function () {



                }, 100);




            }


        );
    </script>
    <script src="~/Scripts/gistfile1.js"></script>
    <script src="~/Scripts/clock-v3.js"></script>
    <style type="text/css">
        .cancelButton {
            cursor: pointer;
        }

